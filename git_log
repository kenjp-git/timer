+        if(this.time[1] == '0') {                            +            min = '00'
+        }else {
+            min = this.time[1];
+        }
+        let datetime = `${this.date[0]}nen ${this.date[1]+1}gatsu ${this.date[2]}nichi ${hour}:${min}`;
+        return datetime;                                     +    }
